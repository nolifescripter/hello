Frame.Parent = gui

-- box
local PlayerNameBox = Instance.new("TextBox")
PlayerNameBox.Size = UDim2.new(0.8, 0, 0.2, 0)
PlayerNameBox.Position = UDim2.new(0.1, 0, 0.2, 0)
PlayerNameBox.PlaceholderText = "Enter player name"
